local Two = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Two:AddToggle({
	Name = "Glove Esp",
	Default = false,
	Callback = function(Value)
		getgenv().Esp = Value
        
        -- Start a coroutine to handle the ESP logic
		spawn(function()
			while getgenv().Esp do
				for _, player in pairs(game.Players:GetChildren()) do
					if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("Head") and player.Character:FindFirstChild("entered") and player.Character.IsInDefaultArena.Value == false then
						local gloveEsp = player.Character.Head:FindFirstChild("GloveEsp")
                        
                        -- Create the ESP only if it doesn't already exist
						if not gloveEsp then
							gloveEsp = Instance.new("BillboardGui", player.Character.Head)
							gloveEsp.Adornee = player.Character.Head
							gloveEsp.Name = "GloveEsp"
							gloveEsp.Size = UDim2.new(0, 100, 0, 150)
							gloveEsp.StudsOffset = Vector3.new(0, 3, 0)
							gloveEsp.AlwaysOnTop = true
							local gloveEspText = Instance.new("TextLabel", gloveEsp)
							gloveEspText.BackgroundTransparency = 1
							gloveEspText.Size = UDim2.new(0, 100, 0, 100)
							gloveEspText.TextSize = 25
							gloveEspText.Font = Enum.Font.SourceSansSemibold
							gloveEspText.TextStrokeTransparency = 0
							gloveEspText.Text = player.leaderstats.Glove.Value
                            
                            -- Condition for glove types and colors
							if player.leaderstats.Glove.Value == "OVERKILL" or player.leaderstats.Glove.Value == "bob" or player.leaderstats.Glove.Value == "rob" or player.leaderstats.Glove.Value == "Error" or player.leaderstats.Glove.Value == "God's Hand" or player.leaderstats.Glove.Value == "The Flex" or player.leaderstats.Glove.Value == "Jebaited" then
								gloveEspText.TextColor3 = Color3.new(1, 0, 0) -- Red color
							elseif player.leaderstats.Glove.Value == "Killstreak" or player.leaderstats.Glove.Value == "Glovel" or player.leaderstats.Glove.Value == "Reverse" or player.leaderstats.Glove.Value == "Barrel" then
								gloveEspText.TextColor3 = Color3.new(1, 1, 0) -- Yellow color
							elseif player.leaderstats.Glove.Value == "Extended" or player.leaderstats.Glove.Value == "sus" then
								gloveEspText.TextColor3 = Color3.new(0, 0, 0) -- Black color
							else
								gloveEspText.TextColor3 = Color3.new(1, 1, 1) -- Default white color
							end
						end
					end
				end
				task.wait() -- Small delay for loop efficiency
			end

            -- Remove all ESP instances when the toggle is turned off
			if not getgenv().Esp then
				for _, player in pairs(game.Players:GetChildren()) do
					if player.Character and player.Character:FindFirstChild("Head") then
						local gloveEsp = player.Character.Head:FindFirstChild("GloveEsp")
						if gloveEsp then
							gloveEsp:Destroy()
						end
					end
				end
			end
		end)
	end 
})

Two:AddToggle({
	Name = "Trap Esp",
	Default = false,
	Callback = function(Value)
		getgenv().TrapEsp = Value
        
        -- Start a coroutine to handle the ESP logic
		spawn(function()
			while getgenv().TrapEsp do
                -- Loop through all objects in the workspace to find all "HitBox" parts
				for _, obj in pairs(workspace:GetChildren()) do
					if obj:IsA("Model") and obj:FindFirstChild("HitBox") then
						local hitBox = obj:FindFirstChild("HitBox")
                        
                        -- Create the ESP only if it doesn't already exist
						if hitBox and not hitBox:FindFirstChild("TrapEsp") then
							local trapEsp = Instance.new("BillboardGui", hitBox)
							trapEsp.Adornee = hitBox
							trapEsp.Name = "TrapEsp"
							trapEsp.Size = UDim2.new(0, 100, 0, 150)
							trapEsp.StudsOffset = Vector3.new(0, 1, 0)
							trapEsp.AlwaysOnTop = true
							local trapEspText = Instance.new("TextLabel", trapEsp)
							trapEspText.BackgroundTransparency = 1
							trapEspText.Size = UDim2.new(0, 100, 0, 100)
							trapEspText.TextSize = 25
							trapEspText.Font = Enum.Font.SourceSansSemibold
							trapEspText.TextStrokeTransparency = 0
							trapEspText.Text = "Trap"
							trapEspText.TextColor3 = Color3.new(1, 0, 0) -- Red color for traps
						end
					end
				end
				task.wait() -- Small delay for loop efficiency
			end

            -- Remove the ESP instance when the toggle is turned off
			if not getgenv().TrapEsp then
                -- Loop through all objects in the workspace to remove "TrapEsp" from all HitBoxes
				for _, obj in pairs(workspace:GetChildren()) do
					if obj:IsA("Model") and obj:FindFirstChild("HitBox") then
						local hitBox = obj:FindFirstChild("HitBox")
						if hitBox then
							local trapEsp = hitBox:FindFirstChild("TrapEsp")
							if trapEsp then
								trapEsp:Destroy()
							end
						end
					end
				end
			end
		end)
	end 
})


