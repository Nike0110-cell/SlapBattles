--- Orion
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
	Name = "My SB GUI",
	HidePremium = false,
	SaveConfig = true,
	ConfigFolder = "OrionTest"
})


-- Create a tab in the GUI
local One = Window:MakeTab({
	Name = "Simple",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

One:AddButton({
	Name = "Get [Redacted]",
	Callback = function()
		if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 5000 then
			Door = 0
			for i = 1, 10 do
				Door = Door + 1
				if game:GetService("BadgeService"):UserHasBadgeAsync(game.Players.LocalPlayer.UserId, 2124847850) then
					Door = nil
				else
					firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door].TouchInterest.Parent, 0)
					firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door].TouchInterest.Parent, 1)
					wait(3.75)
				end
			end
		else
			OrionLib:MakeNotification({
				Name = "Error",
				Content = "You don't have 5000 slap, or you have Owner bagde",
				Image = "rbxassetid://7733658504",
				Time = 5
			})
		end
	end
})


One:AddButton({
	Name = "Get Trap",
	Callback = function()
		local player = game:GetService("Players").LocalPlayer
		player.Character.Humanoid:TakeDamage(player.Character.Humanoid.MaxHealth)
		wait(3)
		fireclickdetector(game.workspace.Lobby.Brick.ClickDetector)
		wait(1)
		local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
		rootPart.CFrame = game:GetService("Workspace").Lobby.Teleport1.CFrame
		
		local player = game.Players.LocalPlayer

		-- Check if the player is dead
		if not player.Character or not player.Character:FindFirstChild("Humanoid") or player.Character.Humanoid.Health <= 0 then
			print("Player is dead. Actions stopped.")
			return
		end

		local rootpart = player.Character:FindFirstChild("HumanoidRootPart")

		if rootpart then
			local brick = true
			local platform = Instance.new("Part")
			platform.Anchored = true
			platform.Size = Vector3.new(21, 7, 21)
			platform.CFrame = CFrame.new(-2000, 100, -2000)
			platform.BrickColor = BrickColor.new("Red")
			platform.Parent = game:GetService("Workspace")

			wait(0.5)

			rootpart.CFrame = CFrame.new(-2000, 109, -2000)
			local brick = true 

			while brick and wait() do
				game:GetService("ReplicatedStorage").lbrick:FireServer()
				game:GetService("Players").LocalPlayer.PlayerGui.BRICKCOUNT.ImageLabel.TextLabel.Text = game:GetService("Players").LocalPlayer.PlayerGui.BRICKCOUNT.ImageLabel.TextLabel.Text + 1
				wait(1.5)
			end
		end
	end
})

