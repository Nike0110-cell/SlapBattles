Two:AddToggle({
	Name = "Fort (Use ZZZ)",
	Default = false,
	Callback = function(Value)
		_G.Fort = Value
		while _G.Fort do
			for i, v in pairs(game.Workspace:GetChildren()) do
				if v.Name == "Part" then
					firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 0)
					firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 1)
				end
			end
			task.wait()
		end
	end
})

Two:AddToggle({
    Name = "Engineer",
    Default = false,
    Callback = function(Value)
        getgenv().Fort = Value

        while getgenv().Fort do
            local Glove = game:GetService("Workspace"):FindFirstChild("Turret") 
                        and game:GetService("Workspace").Turret:FindFirstChild("HingePart") 
                        and game:GetService("Workspace").Turret.HingePart:FindFirstChild("Glove")

            local Feet = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

            if Glove and Feet then
                -- Vibrate the Glove near the player and activate TouchInterest
                for i = 1, 5 do
                    Glove.CFrame = Feet.CFrame * CFrame.new(math.random(-1,1), math.random(-1,1), math.random(-1,1))

                    -- Simulate touch interaction
                    firetouchinterest(Feet, Glove, 0) -- Start touch
                    task.wait(0.05)
                    firetouchinterest(Feet, Glove, 1) -- End touch
                end
            end
            task.wait(0.1) -- Small delay to prevent crashes
        end
    end    
})

Two:AddToggle({
    Name = "Auto Teleport Loop",
    Default = false,
    Callback = function(Value)
        getgenv().TeleportLoop = Value -- Store toggle state

        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local Workspace = game:GetService("Workspace")

        while getgenv().TeleportLoop do
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
            local Humanoid = Character:FindFirstChild("Humanoid")

            if HumanoidRootPart and Humanoid then
                -- Step 1: Teleport to the lobby
                local LobbyTeleport = Workspace:FindFirstChild("Lobby") and Workspace.Lobby:FindFirstChild("Teleport1")
                if LobbyTeleport then
                    HumanoidRootPart.CFrame = LobbyTeleport.CFrame
                end

                task.wait(0.2) -- Small delay before next teleport

                -- Step 2: Teleport to specific coordinates (-12874, 1162, 537)
                HumanoidRootPart.CFrame = CFrame.new(-12874, 1162, 537)

                task.wait(0.2) -- Small delay before moving to the turret

                -- Step 3: Find and teleport to any Glove part inside a Turret
                for _, turret in pairs(Workspace:GetChildren()) do
                    local Glove = turret:FindFirstChild("HingePart") and turret.HingePart:FindFirstChild("Glove")
                    if Glove then
                        HumanoidRootPart.CFrame = Glove.CFrame
                        
                        -- Step 4: Simulate touch event
                        task.wait(0.5)
                        firetouchinterest(HumanoidRootPart, Glove, 0) -- Start touch
                        task.wait(0.5)
                        firetouchinterest(HumanoidRootPart, Glove, 1) -- End touch
                        
                        -- Step 5: Reset the character properly
                        Humanoid.Health = 0 -- This will instantly kill/reset the character

                        break -- Stop searching for more turrets once one is found
                    end
                end
            end

            task.wait(4) -- Delay before restarting the loop
        end
    end
})

Two:AddToggle({
    Name = "Redacted",
    Default = false,
    Callback = function(Value)
        getgenv().Fort = Value
        
        while getgenv().Fort do
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            local humanoid = character:FindFirstChild("Humanoid")

            local teleport1 = game:GetService("Workspace").Lobby:FindFirstChild("Teleport2")
            local redacted = game:GetService("Workspace").WellModel.Paticle

            -- Move Lobby.Teleport2 to the player's position
            if teleport1 and humanoidRootPart then
                teleport1.CFrame = humanoidRootPart.CFrame
            end

            task.wait(1) -- Wait before next step

            -- Move player to WellModel.Hitbox
            if humanoidRootPart and redacted then
                humanoidRootPart.CFrame = redacted.CFrame
            end

            task.wait(3)--Wait before setting health

            -- Set health to 0 to "kill" the player
            if humanoid then
                humanoid.Health = 0
            end

            task.wait(5)--revent excessive looping
        end
    end
})


Two:AddToggle({
    Name = "Killstreak",
    Default = false,
    Callback = function(Value)
        getgenv().Fort = Value
        
        while getgenv().Fort do
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            local humanoid = character:FindFirstChild("Humanoid")

            local teleport1 = game:GetService("Workspace").Lobby:FindFirstChild("Teleport2")
            local KillstreakGlove = game:GetService("Workspace")["0_34b"].Killstreak:FindFirstChild("Glove")

            -- Move Lobby.Teleport2 to the player's position
            if teleport1 and humanoidRootPart then
                teleport1.CFrame = humanoidRootPart.CFrame
            end

            task.wait(1) -- Short delay

            -- Move player to Killstreak.Glove
            if humanoidRootPart and KillstreakGlove then
                humanoidRootPart.CFrame = KillstreakGlove.CFrame
            end

            task.wait(1) -- Shorter wait time for smoother execution

            -- Set health to 0 (kill player)
            if humanoid then
                humanoid.Health = 0
            end

            task.wait(4.7) -- Prevent excessive looping
        end
    end
})

